# Node gateway configuration
AGENT_URL=http://localhost:5001
PORT=4000
JWT_SECRET=change-me

# Together AI (openai-compatible) provider
TOGETHER_API_KEY=your_together_api_key
TOGETHER_BASE_URL=https://api.together.xyz/v1

# DeepSeek provider (openai-compatible)
DEEPSEEK_API_KEY=your_deepseek_key
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1

# Anthropic provider (Claude)
ANTHROPIC_API_KEY=your_anthropic_key
ANTHROPIC_BASE_URL=https://api.anthropic.com

# Gemini provider
GOOGLE_API_KEY=your_gemini_key
GOOGLE_API_BASE_URL=https://generativelanguage.googleapis.com/v1beta

# OpenAI / ChatGPT provider
OPENAI_API_KEY=your_openai_key
OPENAI_BASE_URL=https://api.openai.com/v1

# OpenRouter for multi-provider fanout
OPENROUTER_API_KEY=your_openrouter_key
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Optional local/self-hosted OpenAI-compatible endpoint
LOCAL_OPENAI_BASE_URL=http://localhost:11434/v1
LOCAL_OPENAI_API_KEY=ollama
DEFAULT_MODEL=togethercomputer/Apriel-1.6-15B-Thinker

# Where the agent persists dynamically added providers and routing
CUSTOM_PROVIDER_PATH=./providers.json
ROUTING_CONFIG_PATH=./routing.json
